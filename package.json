{
  "name": "default-bookmark-folder",
  "version": "2.4.1",
  "description": "WebExtension allowing you to choose the default bookmark location folder and quickly bookmark pages there via a dedicated icon.",
  "main": "background.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint ./src",
    "lint": "web-ext lint --source-dir ./src/",
    "start:firefox": "web-ext run --source-dir ./src/ --firefox=firefox --firefox-profile=dev --start-url about:debugging",
    "start:nightly": "web-ext run --source-dir ./src/ --firefox=nightly --firefox-profile=nightly --start-url about:debugging",
    "start:dev": "web-ext run --source-dir ./src/ --firefox=firefoxdeveloperedition --firefox-profile=dev --start-url about:debugging",
    "start:waterfox": "web-ext run --source-dir ./src/ --pre-install --firefox=\"C:\\Program Files\\Waterfox\\Waterfox.exe\" --start-url about:debugging",
    "build": "web-ext build --source-dir ./src/ --overwrite-dest --artifacts-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teddy-gustiaux/default-bookmark-folder.git"
  },
  "keywords": [
    "webextension",
    "javascript",
    "firefox",
    "browser-extension",
    "firefox-extension",
    "bookmarks"
  ],
  "author": "Teddy Gustiaux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teddy-gustiaux/default-bookmark-folder/issues"
  },
  "homepage": "https://github.com/teddy-gustiaux/default-bookmark-folder#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7",
    "web-ext": "^2.7.0"
  }
}
