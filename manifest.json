{
  "manifest_version": 2,
  "name": "Default Bookmark Folder",
  "author": "Teddy Gustiaux",
  "version": "1.0",

  "description": "Allows you to choose the default bookmark location folder.",
  "homepage_url": "https://github.com/teddy-gustiaux/default-bookmark-folder",

  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],

  "icons": {
    "48": "icons/default-bookmark-folder-48.png",
    "96": "icons/default-bookmark-folder-96.png"
  },

  "background": {
    "scripts": ["default-bookmark-folder.js"]
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "applications": {
    "gecko": {
      "id": "default-bookmark-folder@temporary-id.org",
      "strict_min_version": "57"
    }
  }

}